{ 
  "extends": [
    "airbnb",
    "prettier"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
  },
  "rules": {
    "valid-jsdoc": "error",
    "no-nested-ternary": "off",
    "no-use-before-define": "off",
    "no-console": ["error", { "allow": ["log", "warn", "error"] }]
  }
}
